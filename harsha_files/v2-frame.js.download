    /**
     * Integrations
     * @param {type} platform
     * @returns {tidioChat.getIntegration.e|type.integrations|@var;tidioChatData.integrations|type@call;slice.integrations|Boolean}Integrations
     */
    getIntegration: function (platform) {
        if (!tidioChat.chat_data.integrations) {
            return false;
        }
        for (var i = 0, c = tidioChat.chat_data.integrations.length; i < c; ++i) {
            var e = tidioChat.chat_data.integrations[i];
            if (e.platform == platform) {
                try {
                    e.data = $.parseJSON(e.data);
                } catch (e) {
                    e.data = {};
                }
                return e;
            }
        }
        return false;
    },
    trackEvent: function (event_name, _once) {

        // Check if we have google analytics integration turned on
        if (!tidioChat.getIntegration('ga')) {
            return false;
        }
        var event_id = 'trackEvent_' + event_name;
        if (_once && this.storage_local.get(event_id)) {
            return false;
        } else if (_once) {
            this.storage_local.set(event_id, '1')
        }
        // Check support or one of 3 trackers.
        if (typeof parent.window._gaq !== 'undefined') {
            parent.window._gaq.push(['_trackEvent', 'Tidio Chat', event_name]);
        } else if (typeof parent.window.ga !== 'undefined') {
            parent.window.ga('send', 'event', 'Tidio Chat', event_name);
        } else if (typeof parent.window.__gaTracker !== 'undefined') {
            parent.window.__gaTracker('send', 'event', 'Tidio Chat', event_name);
        }
    },
    // Linkify
    _linkify: function (inputText) {
        var replacedText, replacePattern1, replacePattern2, replacePattern3;
        //URLs starting with http://, https://, or ftp://
        replacePattern1 = /(\b(https?|ftp):\/\/[-A-Z0-9+&@#\/%?=~_|!:,.;]*[-A-Z0-9+&@#\/%=~_|])/gim;
        replacedText = inputText.replace(replacePattern1, '<a href="$1" target="_blank">$1</a>');
        //URLs starting with "www." (without // before it, or it'd re-link the ones done above).
        replacePattern2 = /(^|[^\/])(www\.[\S]+(\b|$))/gim;
        replacedText = replacedText.replace(replacePattern2, '$1<a href="http://$2" target="_blank">$2</a>');
        //Change email addresses to mailto:: links.
        replacePattern3 = /(([a-zA-Z0-9\-\_\.])+@[a-zA-Z0-9\_]+?(\.[a-zA-Z]{2,6})+)/gim;
        replacedText = replacedText.replace(replacePattern3, '<a href="mailto:$1">$1</a>');
        return replacedText;
    },
    /**
     * IE Placeholder Fix
     */
    placeholderFix: function () {
        if (tidioFunc.browser().name == 'MSIE' && tidioFunc.browser().version <= 9) {
            $(document).ready(function () {
                $('input, textarea').each(function () {
                    var holder = $(this).attr('placeholder');
                    if (typeof (holder) !== 'undefined') {
                        $(this).val(holder);
                        $(this).bind('click', function () {
                            if ($(this).val() === holder) {
                                $(this).val('');
                            }
                        }).blur(function () {
                            if ($(this).val().length === 0) {
                                $(this).val(holder);
                            }
                        });
                    }
                });
            });
        }
    },
    /**
     * Helper functions
     * @returns {Boolean}
     */
    isMobile: function () { // @todo: move to tidioFunc
        try {
            if (typeof parent.document.tidioForceMobileState != 'undefined') {
