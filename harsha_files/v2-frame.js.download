    /**
     * Get messages via sockets
     * @returns {undefined}
     */
    messagesSocketReload: function () {

        view.showLoading();
        var messages_log = this.storage_local.get('tidioChatLog'),
            time_start = 0;

        if (messages_log && messages_log[0] && messages_log[0]['time']) {
            time_start = messages_log[0]['time'];
        }
        this.socket.emit('visitorGetMessages', {
            visitorId: tidioVisitor.getId(),
            projectPublicKey: tidioChat.project_public_key,
            timeFrom: time_start,
            browserSessionId: tidioVisitor.getBrowserSessionId()
        }, function (data) {
            if (!data.status) {
                view.hideLoading();
                return false;
            }
            messages_log = [];
            for (var i in data.messages) {
                var e = data.messages[i],
                    e_log = null;
                if (!e || e.type == 'log') {
                    continue;
                }
                e_log = {
                    automatic: false,
                    image: null,
                    message: e.message,
                    name: null,
                    time: e.time_sent,
                    type: "user"
                };
                if (e.auto && e.auto == '1') {
                    e_log.automatic = true;
                }
                if (e.type == 'operator') {
                    e_log.type = 'operator';
                }
                messages_log.push(e_log);
            }
            // Replay messages setting logs, remove html code, and replay messages
            tidioChat.storage_local.set('tidioChatLog', messages_log);
            $('#tidio-chat-discussion .message-wrapper').remove();
            tidioChat.log = null;
            tidioChat.logMessageReplay();

            view.hideLoading();

            tidioChat.socket.emit('visitorReadMessages', {
                visitorId: tidioVisitor.getId(),
                projectPublicKey: tidioChat.project_public_key
            });
        });
    },
