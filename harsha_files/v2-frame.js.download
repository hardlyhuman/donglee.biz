        if (html_departments) {
            html +=
                '<select data-type="department">' +
                '<option value="">' + trans('chat.departmentSelect') + '</option>' +
                html_departments +
                '</select>';
        }
        for (var i = 0, c = fields.length; i < c; ++i) {
            var e = fields[i];
            var placeholder = trans('chat.preformInput_' + e.type);
