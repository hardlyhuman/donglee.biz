    /**
     * Remove if there messages are older than 24h.
     * We are limited with amount of browser memory.
     */
    logMessagesRemoveOld: function () {
        // Remove if there messages are older than 24h
        var logs = tidioChat.storage_local.get('tidioChatLog'),
            new_logs = [];
        if (!logs) {
            return false;
        }
        for (i in logs) {
            if (!logs[i] || tidioFunc.time() - 24 * 3600 > logs[i]['time']) {
                continue;
            }
            new_logs.push(logs[i]);
        }
        if (logs.length != new_logs.length) {
            tidioChat.storage_local.set('tidioChatLog', new_logs);
            this.remove_old_messages = true;
        }
