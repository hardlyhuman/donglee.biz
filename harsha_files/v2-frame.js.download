    // Get number of messages from current visitor in some time
    numberOfMessagesFromInLast: function(seconds, type){
        var maximum_time = tidioFunc.time() - seconds;
        var number_of_messages = 0;
        var chats = tidioChat.storage_local.get('tidioChatLog'); // get all chats
        if(!chats || !chats[0]){
            return 0;
        }
        chats = chats.reverse();
        for(var i=0,c=chats.length;i<c;++i){
            if(!chats[i]){
                continue;
            }
            if(chats[i]['time'] < maximum_time){
                break;
            }
            if(chats[i]['type']==type && !chats[i]['automatic']){
                number_of_messages++;
            }
        }
        return number_of_messages;
    },
    /**
     * Connect cache is using to caching state of chat.
     * It is used to get cached state of chat before we connect via sockets.
     * Response time is much faster after website was laoded.
     */
    connectCacheStateGet: function () {
        var connect_status = this.storage_local.get('tidioChatConnectStatus');
        if (!connect_status) {
            return null;
        }
        if (connect_status.time > tidioFunc.time() - 5 * 60) {
            return connect_status.data;
        }
        return null;
    },
    connectCacheStateSet: function (d) {
        this.storage_local.set('tidioChatConnectStatus', {
            data: d,
            time: tidioFunc.time()
        });
    },
    connectCacheStateRefresh: function () {
        var connect_status = this.storage_local.get('tidioChatConnectStatus');
        if (!connect_status) {
            return null;
        }
        connect_status.data.projectStatus = tidioChat.status_true;
        connect_status.time = tidioFunc.time();
        this.storage_local.set('tidioChatConnectStatus', connect_status);
    },
    connectCacheStateAutoRefresh: function () {
        if (this._connect_cache_auto_refresh) {
            return false;
        }
        this._connect_cache_auto_refresh = true;
        setInterval(function () {
            tidioChat.connectCacheStateRefresh();
