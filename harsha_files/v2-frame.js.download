$.fn.scrollBottom = function (animate) {
    if (typeof animate == 'undefined') {
        animate = false;
    }
    var _this = this;
    if (!_this[0]) {
        return false;
    }
    var scrollHeight = _this[0].scrollHeight;
    if (scrollHeight - _this.scrollTop() > 1000)
        scrollHeight *= 2;
    if (animate) {
        _this.delay(300).stop().animate({scrollTop: scrollHeight}, 2000, "easeOutExpo");
        setTimeout(function () {
            _this.stop();
        }, 400);
    } else {
        _this.scrollTop(scrollHeight);
    }
    return this;
};
/**
