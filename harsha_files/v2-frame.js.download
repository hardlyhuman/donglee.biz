    },
    hidePopup: function (without_animation) {

        tidioChat.popup_open = false;
        tidioChat.storage_cookie.set('tidioChatOpen', '0');

        var $element = $('.tidio-chat-window:visible');
        // Fix form preform button
        $element.find('input[data-lang="preformButton"]').hide();

        var opacity = $element.css('opacity');
        // If we are in middle of animation we do not nothing
        if (opacity > 0 && opacity < 1) {
            return;
        }
        if (without_animation || tidioChat.isMobile()) {
            $element.find('#tidio-chat-discussion').css({
                opacity: 0
            });
            $('#tidio-chat-button').css({
                opacity: 1
            });
            $element.css({
                height: 44,
            });
            $element.animate({'opacity': 0}, 'fast', 'linear', function () {
                $element.hide();
                tidioChat.resizeIframe();
            });
        } else {
            $element.find('#tidio-chat-discussion').animate({
                opacity: 0
            }, 'fast');
            $('#tidio-chat-button').animate({
                opacity: 1
            }, 'fast');
            $element.stop().animate({
                height: 110,
                opacity: 0
            }, 300, 'easeOutExpo', function () {
                $element.hide();
                tidioChat.resizeIframe();
            });
        }
        tidioChat.api('on', 'popUpHide');
        tidioChat.disconnectModeCheck();
    },
    showOperatorTyping: function () {
        if ($("#operator-is-typing").length) {
            return false;
        }
        var $typingMessage = view.messageTemplate({
            id: 'operator-is-typing',
            type: 'operator',
            message: trans('chat.operatorIsTyping'),
        });
        $('#tidio-chat-discussion').append($typingMessage);
        $typingMessage.removeClass('beforeshow');
        if (tidioChat.getOpenStatus()) {
            view.showPopup(true);
        }
    },
    /**
     *
     * @param hide_fast
     */
    hideOperatorTyping: function (hide_fast) {
        // We need to do it fast because of recalculation of the style
        $('#operator-is-typing').remove();
        if (tidioChat.getOpenStatus()) {
            view.showPopup(true);
        }
    },
    setOperatorImage: function (image_path) {
        $(".tidio-chat-window .avatar").css({
            'background-image': 'url(' + image_path + ')'
        });
        $(".tidio-chat-window .image").css({
            'background-image': 'url(' + image_path + ')'
        });
        $("#tidio-chat-button .avatar").css({
            'background-image': 'url(' + image_path + ')'
        });
        $("#tidio-chat-button .image").css({
            'background-image': 'url(' + image_path + ')'
        });
        $(".browser-ie8  .tidio-chat-window .image").css("background-size", "cover");
        $(".browser-ie8  #tidio-chat-button .image").css("background-size", "cover");
    },
    setOperatorData: function (name, status) {
        $('[data-element="chatOperatorName"]').text(name);
        $('[data-element="chatOperatorStatus"]').html(status);
    },
    // Offline Message Alert
    offlineMessageAlert: function (msg, status) {
        if (typeof status == 'undefined')
            status = '';
        var $alert = $("#tidio-chat-window-offline-form-alert");
        if ($alert.length) {
            $alert.html(msg).addClass(status);
            $alert.show().css('top', -$alert.outerHeight()).hide().slideDown('fast');
        } else {
            alert(msg);
        }
    },
    /**
     * Add new message into chat log
     * @param {type} data
     * @returns {String}
     */
    appendMessage: function (data) {
        var $discussion = $('#tidio-chat-discussion');
        var $newMessage = view.messageTemplate(data);
        view.hideIntroMessage(true);
        // Add "disableanimation" class to existed messages
        var $oldMessages = $discussion.find('.message-wrapper').addClass('disableanimation');
        // Append new message with beforeshow class and scroll bottom
        $discussion.append($newMessage).scrollBottom();
        var newMessageHeight = $newMessage.outerHeight();
        $oldMessages.css({
            bottom: -newMessageHeight
        });
        setTimeout(function () {
            $discussion.find('.message-wrapper').removeClass('beforeshow disableanimation');
            $oldMessages.css({
                bottom: 0
            });
        }, 0);

        // It is needed for state when widget is not in full height
        if (tidioChat.getOpenStatus()) {
            view.showPopup(true);
        }
        $(".browser-ie8  .operator .avatar-placeholder").css("background-size", "cover");
    },
    // Message Template
    messageTemplate: function (data) {
        if (typeof data == 'string') {
            data = {
                message: data
            }
        }
        if (!data.time) {
            data.time = tidioFunc.time();
        }
        var html_footer = '';
        html_footer = tidioChat.getTimeMinute(data.time);
        if (data.name) {
            html_footer += ' - ' + data.name;
        }

        var attr_avatar = '';
        if (data.type == 'operator') {
            var attr_avatar = 'style=""';
            if (data.image) {
                attr_avatar = 'style="background-image: url(' + data.image + '); background-size: cover !important;"';
            } else if (tidioChat.chat_data['image']) {
                attr_avatar = 'style="background-image: url(' + tidioChat.chat_data['image'] + ');"';
            }
        }
        var template = $('#message-template').html();
        data.message = tidioChat._linkify(data.message);
        var html = Mustache.render(template, {
            data: data,
            attr_avatar: attr_avatar,
            html_footer: html_footer
        });

        html = view.replaceImageLinks(html);
        var $html = $(html);
        $html.addClass('beforeshow');
        return $html;
    },
    replaceImageLinks: function (html) {
        var $html = $(html);
        $html.find('.message a').each(function () {
            var url = $(this).attr('href');
            if (/\.(jpeg|jpg|gif|png)$/.test(url)) {
                var $img = $('<img src="' + url + '">');
                if (tidioChat.popup_open === true) {
                    $img.load(function () {
                        view.showPopup(true);
                    });
                }
                $(this).html($img);
                $(this).addClass('image-preview');
            }
        });
        return html = $html.clone().wrap('<div></div>').parent().html();
    },
    /**
     * Rating actions
     */
    ratingShow: function () {
        $('#tidio-chat-rating').fadeIn('fast');
        $('#settings').removeClass('active');
    },
    ratingHide: function () {
        $('#tidio-chat-rating').fadeOut('fast');
    },
    ratingChoosen: function (rate) {
        $(".tidio-chat-rating-good,.tidio-chat-rating-bad").removeClass('active');
        if (rate == '0') {
            $(".tidio-chat-rating-bad").addClass('active');
        }
        if (rate == '1') {
            $(".tidio-chat-rating-good").addClass('active');
        }
        $('.tidio-chat-rating-comment-wrapper').addClass('active');
    },
    showLoading: function () {
        $("#tidio-chat-discussion").addClass('loading');
    },
    hideLoading: function () {
        $("#tidio-chat-discussion").removeClass('loading');
    },
    hideIntroMessage: function (without_animation) {
        if (this._intro_hidden) {
            return false;
        }
        this._intro_hidden = true;
        var $message = $(".intro-message");
        var $messageEl = $message.find('.message, .avatar');

        if (without_animation) {
            $messageEl.hide();
            $message.hide();
        } else {
            $messageEl.animate({
                'opacity': 0,
                'bottom': 20
            }, function () {
                $(this).hide();
                $message.hide();
                // Reinitialize view after intro message was hidden
                if (tidioChat.getOpenStatus()) {
                    view.showPopup(true);
                }
            });
        }
    },
};
$(document).ready(function () {
    view.init();
});

