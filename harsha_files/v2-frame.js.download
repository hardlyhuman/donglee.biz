    /**
     * Opening Hours
     */
    isOpeningHours: function () {

        if (!tidioChat.chat_data.opening_hours) {
            return false;
        }
        var opening_hours = tidioChat.chat_data.opening_hours;
        var date = new Date();

        //

        if (tidioChat.chat_data.timezone && typeof tidioChat.chat_data.timezone == 'string') {
            tidioChat.chat_data.timezone = JSON.parse(tidioChat.chat_data.timezone);
        }

        if (typeof tidioChat.chat_data.opening_hours == 'string') {
            tidioChat.chat_data.opening_hours = JSON.parse(tidioChat.chat_data.opening_hours);
        }

        if (tidioChat.chat_data.timezone && tidioChat.chat_data.timezone[2] && _momentjs) { // new method of get time zone
            return this.isOpeningHoursMomentJS();
        } else if (typeof tidioFunc.getOffsetTime == 'function' && tidioChat.chat_data.timezone) { // old method of get timezone
            date = new Date(tidioFunc.getOffsetTime(tidioChat.chat_data.timezone[1]));
        }
        var now_day = date.getDay();
        var now_hour = date.getHours() * 60 + date.getMinutes() * 1;

        // because 0 is sunday, and we use 7 as sunday
        if (now_day == 0) {
            now_day = 7;
        }
        now_day -= 1;

        for (var i = 0, c = opening_hours.length; i < c; ++i) {
            var e = opening_hours[i];
            if (now_day == i) {
                if (e[0] == 0 && e[1] == 0) {
                    return true; // this is a opening hours time
                }
                if (now_hour > e[0] && now_hour < e[1]) {
                    return false;
                } else {
                    return true;
                }
            }
        }
        return false;
    },
    isOpeningHoursMomentJS: function () {

        if (!tidioChat.chat_data.opening_hours) {
            return false;
        }
        _momentjs.tz.add(tidioChat.chat_data.timezone[2]);
        var now_moment = _momentjs().tz(tidioChat.chat_data.timezone[0]);
        var now_day = now_moment.format('d');
        var now_hour = now_moment.format('H') * 60 + now_moment.format('m') * 1;

        if (now_day == 0) { // because 0 is sunday, and we use 7 as sunday
            now_day = 7;
        }
        now_day -= 1;

        var opening_hours = tidioChat.chat_data.opening_hours;
        for (i in opening_hours) {
            var e = opening_hours[i];
            if (now_day == i) {
                if (e[0] == 0 && e[1] == 0) {
                    return true; // this is a opening hours time
                }
                if (now_hour > e[0] && now_hour < e[1]) {
                    return false;
                } else {
                    return true;
                }
            }
        }
        return false;
    },
    autoRefreshAfterOpeningHours: function (callback) {

        if (tidioChat._auto_refresh_opening_hours) {
            return false;
        }
        tidioChat._auto_refresh_opening_hours = setInterval(function () {
            tidioFunc.log('autoRefreshAfterOpeningHours', 'check');
            var opening_hours_status = tidioChat.isOpeningHours();
            if (!opening_hours_status) {
                callback();
                clearInterval(tidioChat._auto_refresh_opening_hours);
            }
        }, 10 * 1000);
    },
    /*
     ** Opening Hours - Times
     */

    initOpeningHours: function () {

        // There is no opening hours settings so we should not display it at all
        if (!tidioChat.chat_data.opening_hours || !_momentjs) {
            return false;
        }

        var html = this.openingHoursRenderHours();
        $('.message-hours ul').html(html);

        // Get next available time string
        html = this.openingHoursRenderNextAvailable();
        $('.message-hours > div > span').html(html);
    },
    openingHoursRenderHours: function () {

        var html = '';
        _momentjs.tz.add(tidioChat.chat_data.timezone[2]);
        var opening_hours = tidioChat.chat_data.opening_hours;
        for (var i in opening_hours) {
            var day = opening_hours[i];
            // Get time in minutes
            var time_start = day[0];
            var time_end = day[1];
            // Prepere html for day
            if (time_start == 0 && time_end == 0) { // Offline
                html += '<li>' + trans('chat.day' + i) + ':<span class="right">' + trans('chat.closed') + '</span></li>';
            } else {
                // Convert time with proper timezone - move to time in gmt0 and to browser timezone?
                time_start = this._openingHoursLocalizeTime(time_start);
                time_end = this._openingHoursLocalizeTime(time_end);
                // Format time
                time_start = this._openingHoursRenderTime(time_start);
                time_end = this._openingHoursRenderTime(time_end);
                html += '<li><span>' + trans('chat.day' + i) + '</span>:<span class="right">' + time_start + '-' + time_end + '</span></li>';
            }
        }
        return html;
    },
    _openingHoursLocalizeTime: function (time) {
        return time + tidioChat.chat_data.timezone[1] - new Date().getTimezoneOffset();
    },
    _openingHoursRenderTime: function (timeInMinutes) {
        var hour = timeInMinutes / 60;
        var nextDay = '';
        if (hour > 24) {
            hour -= 24;
            nextDay = ' next day';
        }
        var time = _momentjs({
            hour: hour,
            minute: timeInMinutes % 60
        }).format('HH:mm');
        return time;
    },
    /**
     * Render next available time
     * @returns {String}
     */
    openingHoursRenderNextAvailable: function () {
        var html = '';
        var opening_hours = tidioChat.chat_data.opening_hours;
        var now_moment = _momentjs().tz(tidioChat.chat_data.timezone[0]);
        var now_day = now_moment.format('d');
        var now_hour = now_moment.format('H') * 60 + now_moment.format('m') * 1;

        if (now_day == 0) { // because 0 is sunday, and we use 7 as sunday
            now_day = 7;
        }
        now_day -= 1;
        html = trans('chat.willBeAvailable') + ' ';
        var time_start_today = opening_hours[now_day][0]
        time_start_today = this._openingHoursLocalizeTime(time_start_today);

        // For today condition
        if (opening_hours[now_day][0] > now_hour) {
            var hour = this._openingHoursRenderTime(time_start_today);
            html = trans('chat.willBeAvailableSoon')
                .replace(':period', trans('chat.today'))
                .replace(':time', hour)
        } else {
            // Else for other days
            // Get next available day
            var next_day = now_day + 1;
            var tomorrow_day = now_day + 1;
            if (tomorrow_day == 7) {
                tomorrow_day = 0;
            }
            while (1) {
                if (next_day == 7) {
                    next_day = 0;
                }
                // Condition if we not found any available data
                if (next_day == now_day) {
                    html = trans('chat.willNotBeAvailable');
                    break;
                }
                var time_start_next_day = opening_hours[next_day][0],
                    time_end_next_day = opening_hours[next_day][1];
                // Condition for day with offline
                if (time_start_next_day == 0 && time_end_next_day == 0) {
                    next_day++;
                    continue;
                }
                time_start_next_day = this._openingHoursLocalizeTime(time_start_next_day);
                var hour = this._openingHoursRenderTime(time_start_next_day);
                // Condition for tommorow
                if (next_day === tomorrow_day) {
                    html = trans('chat.willBeAvailableSoon')
                        .replace(':period', trans('chat.tommorow'))
                        .replace(':time', hour)
                } else {
                    html = trans('chat.willBeAvailableDayOfWeek')
                        .replace(':day', trans('chat.day' + next_day))
                        .replace(':time', hour)
                }
                break;
            }
        }
        return html;
    },

    /*
     ** Reload Chat Widget
     */
    reloadWidget: function(){
        if (!parent.document.tidioRefreshedId) {
            parent.document.tidioRefreshedId = true;
            // Should be rewrite, this will work only when we give function in a string (for some reason)
            parent.setTimeout('document.tidioChatRender.iFrameCreate();', 100);
            parent.setTimeout('parent.document.tidioRefreshedId = false;', 500);
