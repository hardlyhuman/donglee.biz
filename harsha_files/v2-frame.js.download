        if (this._connect_listeners) {
            return false;
        }
        this._connect_listeners = true;
        this.socket.on('conversationStream', tidioChat.onConversationStream);
        // @comment: for some reason it is not running
        this.socket.on('projectDataUpdate_' + this.project_public_key, tidioChat.onProjectDataUpdate);
