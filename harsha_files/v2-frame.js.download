    /**
     * Get message from browser storage and put it into view
     */
    logMessageReplay: function () {
        if (this.log !== null) { // return false it was replayed!
            return false;
        }
        this.log = tidioChat.storage_local.get('tidioChatLog');
        if (!this.log) {
            this.log = [];
        }
        // If there is a unread messages from mobile device we gonna put it
        if (window.parent.document.tidioChatMobileUnreadMessages) {
            var unread_msg = window.parent.document.tidioChatMobileUnreadMessages;
            for (i in unread_msg) {
                var e = unread_msg[i];
                this.log.push({
                    type: 'user',
                    message: e.message,
                    time: e.time,
                    image: null,
                    name: e.operatorName
                });
            }
        }
        if (!this.log.length) {
            return false;
        }
        for (var i = 0, c = this.log.length; i < c; ++i) {
            var e = this.log[i];
            if (!e) {
                continue;
            }
            view.appendMessage({
                type: e.type,
                message: e.message,
                time: e.time,
                image: e.image,
                name: e.name
            })
        }
        if (tidioChat.preform_filled === false) {
            tidioChat.preform_filled = 0;
        }
