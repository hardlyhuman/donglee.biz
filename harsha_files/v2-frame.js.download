    onConversationStream: function (data) {
        tidioFunc.log('conversationStream', data);
        if (data.type == 'chat.messageFromOperator') {
            tidioChat.onMessageFromOperator(data.data);
            tidioChat.socket.emit('visitorReadMessages', {
                visitorId: tidioVisitor.getId(),
                projectPublicKey: tidioChat.project_public_key
            });
        }
        if (data.type == 'chat.operatorIsTyping') {
            tidioChat.onOperatorIsTyping(data.data);
        }
        if (data.type == 'chat.start') {
            tidioChat.trackEvent('Chat started');
        }
        if (data.type == 'chat.end') {
            tidioChat.trackEvent('Chat finished');
        }
        if(data.type == 'chat.visitorIsBanned'){
            tidioChat.trackEvent('Visitor banned');
            parent.location.reload();
        }
        if(data.type == 'chat.visitorMerged'){
            tidioChat.visitorMerged(data.data.visitorSourceId, data.data.visitorTargetId);
        }
        if(data.type == 'chat.visitorUnmerged'){
            tidioChat.visitorUnmerged(data.data.visitorSourceId, data.data.visitorSourceBrowserSessionId);
