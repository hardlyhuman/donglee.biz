    /**
     * This is the way how we detect if user is active on a site
     * @todo need optimalization
     * @returns {Boolean}
     */
    visitorActivityListener: function () {
        if (this.visitor_activity_listener) {
            return false;
        }
        this.visitor_activity_listener = true;

        // Set last time activity
        $(window.parent).on('scroll', function () {
            tidioChat.visitor_activity_time = tidioFunc.time();
        });
        $(window.parent).on('mousemove', function () {
            tidioChat.visitor_activity_time = tidioFunc.time();
        });
        $(window).on('scroll', function () {
            tidioChat.visitor_activity_time = tidioFunc.time();
        });
        $(window).on('mousemove', function () {
            tidioChat.visitor_activity_time = tidioFunc.time();
        });
        $('#tidio-online-form-message').on('keyup', function () {
            tidioChat.visitor_activity_time = tidioFunc.time();
        });
        setInterval(function () {
            if (typeof document.hidden == 'boolean' && !document.hidden) {
                tidioChat.visitor_activity_time = tidioFunc.time();
            }
        }, 500);
        setInterval(function () {
            // if project is not online we have to ignore this
            if (!tidioChat.project_status || tidioChat.project_status != 'online') {
                return false;
            }
            if (tidioChat.visitor_activity_time + 90 * 60 > tidioFunc.time()) {
                if (!tidioChat.visitor_activity_status) { // change status to active
                    tidioChat.visitor_activity_status = true;
                    tidioChat.socketLastSeenUpdate(true);
                }
            } else {
                if (tidioChat.visitor_activity_status) { // change status to inactive
                    tidioChat.visitor_activity_status = false;
                }
            }
        }, 10 * 1000);
    },
