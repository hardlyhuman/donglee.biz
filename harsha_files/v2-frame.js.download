    initPreForm: function () {
        this.preFormAddEmailInVersion2();
        this.preFormIdentifyAutoFill();
        //
        var preform = this.chat_data.preform;
        if (!preform || !preform.display || !preform.fields || !preform.fields.length) {
            tidioChat.preform_filled = -1;
            return false;
        }

        if (tidioChat.storage_cookie.get('tidioChatPreFormData')) {
            tidioChat.preform_filled = true;
            return true;
        }

        tidioChat.preform_filled = false;

        //

        var $fields = $(tidioChat.preFormFieldsHtml(preform.fields));
        $("#tidio-chat-window-preform-form").prepend($fields);
        $('#intalk-preform .inchat-dialog-fields').prepend($fields.clone().not('textarea'));

