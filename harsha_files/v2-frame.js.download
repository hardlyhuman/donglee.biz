    /**
     * Send File
     * @todo move jquery to view
     */
    initFileSend: function () {
        if (typeof FormData == 'undefined') {
            $('#send-file-btn').hide();
        } else {
            $('#send-file-btn').on('click', function () {
                var elem = document.getElementById('file');
                if (elem && document.createEvent) {
                    var evt = document.createEvent("MouseEvents");
                    evt.initEvent("click", true, false);
                    elem.dispatchEvent(evt);
                }
                $('#settings').toggleClass('active');
            });
            $('#file').on('change', function (event) {

                //clear input file by replacing it
                var input = $(this);
                input.replaceWith(input = input.clone(true));

                var file = event.target.files[0];
                if (file.size / 1024 / 1024 > 10) {
                    alert('Sorry, only files to 10 megabytes.');
                    return;
                }

                $('#settings-btn').addClass('loading');
                var data = new FormData();
                data.append('upload', file);
                $.ajax({
                    url: tidioChat.source_url + 'upload/file/external?projectPublicKey=' + tidioChat.project_public_key,
                    type: 'POST',
                    data: data,
                    cache: false,
                    dataType: 'json',
                    processData: false, // Don't process the files
                    contentType: false, // Set content type to false as jQuery will tell the server its a query string request
                    success: function (data, textStatus, jqXHR) {

                        if (typeof data != 'object')
                            data = JSON.parse(data);

                        if (data.status === true) {
                            var url = data.value.url;
                            var filename = data.value.fileName;
                            tidioChat.onMessageFromVisitor(url);
                        } else {
                            if (data.value == 'ERR_INVALID_EXTENSION') {
                                alert('Sorry, wrong file format.');
                            } else if (data.value == 'ERR_FILE_SIZE') {
                                alert('Sorry, the file you are trying to send is too big.');
                            } else {
                                alert(data.error);
                            }
                        }
                        $('#settings-btn').removeClass('loading');
                    },
                    error: function (jqXHR, textStatus, errorThrown) {
                        // Handle errors here
                        tidioFunc.log('ERRORS: ' + textStatus);
                        $('#settings-btn').removeClass('loading');
                    }
                });
            });
        }
    },

