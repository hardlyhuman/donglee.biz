            // Use of vanilla js because of ff bug - newsletter elements was not showed at this point
            document.getElementsByClassName('newsletter')[0].style.display = 'block';
            document.getElementsByClassName('newsletter')[1].style.display = 'block';
        }

        $("#tidio-chat-window-preform-form, #intalk-preform").on('submit', function () {

            var data = {};
            var form_error = [];
            var $this = $(this);

            $this.find("[data-type]").each(function () {
                var this_type = this.getAttribute('data-type');
                var this_value = this.value.trim();
                // Valid email
                if (!this_value) {
                    form_error.push([this, '']);
                } else if (this_type == 'email' && this_value.indexOf('@') == -1) {
                    form_error.push([this, '']);
                }
                data[this_type] = this_value;
            });

            if (form_error.length) {
                $(this).find(".input-err").removeClass('input-err');
                for (var i = 0, c = form_error.length; i < c; i++) {
                    $(form_error[i][0]).addClass('input-err');
                }
                return false;
            }

            var email = $(this).find('[data-type="email"]').val();

            if ($this.find('.newsletter input').is(':checked')) {
                tidioChat.preFormSignUpOnNewsletter(email);
            }

            // Process and update view
            var preform_send_message = false;
            if ($(this).attr('id') == 'intalk-preform') {
                preform_send_message = $("#tidio-online-form-message").val();
                //
                $('#intalk-preform').fadeOut();
                $("#tidio-online-form-message").val('');
            }
            tidioChat.processPreForm(data, preform_send_message);
            tidioChat.trackEvent('Pre-chat survey filled', true);
            view.showPopup(true);
            return false;
        });

        $("#tidio-chat-window-preform-form").find('textarea,input').on('blur', function () {
            var $this = $(this);
            var this_value = $this.val();
            if (this_value && $this.hasClass('input-err')) {
                $this.removeClass('input-err');
            }
        });
