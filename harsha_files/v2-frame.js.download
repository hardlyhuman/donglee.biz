    /*
     ** Connection data
     */

    connectionData: function () {

        if (this.connectionDataCacheGet() && !this.debug) {
            return this.connectionDataCacheGet();
        }

        // Set conn data

        var conn_data = {
            host: 'socket-chat-de1.tidio.co',
            port: 443,
            key: tidioFunc.generateHash(),
            url: null,
            protocol: 'https'
        };

        // Get servers
        var servers = null;

        if (parent.document.tidioChatRender && parent.document.tidioChatRender.servers) {
            servers = parent.document.tidioChatRender.servers;
        }

        // Detecting visitor language
        var visitor_location = this._browserLanguageLocation();

        // Detecting based on server
        if (this.debug && this.debug.socket_url) {
            socket_url_parse = tidioFunc.parseURL(this.debug.socket_url);
            //
            conn_data.protocol = socket_url_parse.scheme;
            conn_data.host = socket_url_parse.host;
            conn_data.port = socket_url_parse.port;
        } else if (servers) {
            var server_location = servers['default'];
            if (servers['locations'][visitor_location]) {
                server_location = servers['locations'][visitor_location];
            }
            var cluster = servers['clusters'][server_location];
            conn_data.host = this._randomArray(cluster);
        }

        // Exception for tidio chat project to test ie server, todo: remove it after tests
        /*if(this.project_public_key=='w0i4b7fdeerfqqn4w8lwb7ahjasjnrmd'){
            conn_data = {
                host: 'socket-chat-ie1-test.tidio.co',
                port: 443,
                key: tidioFunc.generateHash(),
                url: null,
                protocol: 'https'
            };
        }*/

        // Connection url
        if (conn_data.port != 443) {
            conn_data.protocol = 'http';
        }

        conn_data.url = conn_data.protocol + '://' + conn_data.host + ':' + conn_data.port + '?key=' + conn_data.key;
        this.connectionDataCacheSet(conn_data);
        return conn_data;
    },

    connectionDataCacheSet: function (conn_data) {
        tidioChat.storage_local.set('cacheConnectionData', conn_data);
    },

    connectionDataCacheGet: function () {

        if (tidioChat.storage_local.get('cacheConnectionData')) {
            return tidioChat.storage_local.get('cacheConnectionData');
        }

        return false;

    },

    connectionDataCacheClear: function () {
        if (!tidioChat.storage_local.get('cacheConnectionData')) {
            return false;
        }

        tidioChat.storage_local.remove('cacheConnectionData');

        return true;
    },

    _randomArray: function (list) {
        return list[Math.floor((Math.random() * list.length))];
    },
    _browserLanguageLocation: function () {
        var browser_language = 'en';

        if (navigator && navigator.language) {
            browser_language = navigator.language;
        } else if (navigator && navigator.userLanguage) {
            browser_language = navigator.userLanguage;
        }

        browser_language = browser_language.toLowerCase();

        if (browser_language.indexOf('-') > -1) {
            browser_language = browser_language.split('-');
            browser_language = browser_language[1];
        }

        return browser_language;
    },
    changedClustersReconnect: function(){
      tidioChat.connectionDataCacheClear();
      var conn_data = this.connectionData();
      this.socket = io.connect(conn_data.url, this.connection_preferences);
    },
    tryReconnect: function() {
        tidioChat.reconnecting.server_down = true;
        // tidioFunc.log('my group is', this.visitorIdToInt(tidioVisitor.getId()));
        var region_lang = tidioChat.chat_lang || 'en';
        var ajax_timeout = null;
        $.ajax({
          cache: false,
          url: tidioChat.reconnecting.crash_file,
          dataType: 'json',
          error: function(a) {
              ajax_timeout = setTimeout(function() {
                  // console.log('socket ajax timedout as per settings. trying to connect');
                  app.socket.connect();
              }, this.reconnecting.ajax_time);
          }.bind(this),
          success: function(a) {
            clearTimeout(ajax_timeout);
            var was_changed = false;
            if (parent.document.tidioChatRender.servers.changed !== a.socket.servers.changed) {
                parent.document.tidioChatRender.servers.changed = a.socket.servers.changed;
                if (a.socket && a.socket.servers) {
                  if(a.socket.servers.clusters) {
                      was_changed = true;
                      parent.document.tidioChatRender.servers.clusters = a.socket.servers.clusters;
                  }
                  if(a.socket.servers.locations) {
                      was_changed = true;
                      parent.document.tidioChatRender.servers.locations = a.socket.servers.locations;
                  }
                  if(a.socket.servers.default) {
                      was_changed = true;
                      parent.document.tidioChatRender.servers.default = a.socket.servers.default;
                  }
                }
            }
            if (a.socket && a.socket.fails) {
                if (!a.socket.allows) {
                    this.reconnecting.reconnect_data.allow_all += 1;
                    if (this.reconnecting.reconnect_data.allow_all < this.reconnecting.reconnect_max_tries) {
                        tidioFunc.log('all users are allowed. trying to reconnect');
                        if (was_changed) {
                            this.changedClustersReconnect.apply(this);
                        }
                        this.socket.connect();
                    } else {
                        // tidioFunc.log('all users are allowed but could not reconnect');
                        this.reconnecting.reconnect_data.allow_all = 0;
                        this.showOfflineMessage(a.socket.message, region_lang);
                    }
                } else if(a.socket.allows  && a.socket.allows.indexOf(this.visitorIdToInt(tidioVisitor.getId())) !== -1) {
                    this.reconnecting.reconnect_data.allow_visitor += 1;
                    if (this.reconnecting.reconnect_data.allow_visitor < this.reconnecting.reconnect_max_tries) {
                        tidioFunc.log('user is in allowed array. Trying to connect');
                        if (was_changed) {
                            this.changedClustersReconnect.apply(this);
                        }
                        this.socket.connect();
                    } else {
                      // tidioFunc.log('user is in allowed array but could not reconnect');
                      this.reconnecting.reconnect_data.allow_visitor = 0;
                      this.showOfflineMessage(a.socket.message, region_lang);
                    }
                } else {
                    tidioFunc.log('user is not allowed, download new file in few seconds');
                    this.showOfflineMessage(a.socket.message, region_lang);
                }
            } else {
                this.reconnecting.reconnect_data.no_error += 1;
                if (this.reconnecting.reconnect_data.no_error < this.reconnecting.reconnect_max_tries || was_changed) {
                    tidioFunc.log('server should be running, trying to reconnect');
                    setTimeout(function() {
                      if (was_changed) {
                          this.changedClustersReconnect.apply(this);
                      }
                      this.socket.connect()
                    }.bind(this), this.reconnecting.reconnect_timeout);
                } else {
                    // console.warn('server should be running but could not reconnect');
                    this.reconnecting.reconnect_data.no_error = 0;
                    this.showOfflineMessage(a.socket.message, region_lang);
                }
            }
          }.bind(this)
        });
    },
    showOfflineMessage: function(messages, region) {
      // show popup and set text if popup was visible
      if (tidioChat.$iframe_document.find("#tidio-chat:visible") && tidioChat.$iframe_document.find("#tidio-chat:visible").length && tidioChat.getOpenStatus()) {
          // set custom text?
          var errorMessage = 'We are experiencing a temporary connectivity issues with one of the servers in your area. Please hold on, the connection will be resumed shortly.';
          if (messages && messages[region] && messages[region].length > 0) { //try error in user language
              errorMessage = messages[region];
          } else if(messages && messages['en'] && messages['en'].length > 0) { //default to en
              errorMessage = messages['en'];
          }
          $("#tidio-chat-serveroffline-box p").text(errorMessage);
          $("#tidio-chat-serveroffline").show();
      }
      setTimeout(function() {
          tidioFunc.log('Reconnecting');
          this.tryReconnect();
      }.bind(this), tidioChat.reconnecting.reconnect_timeout);
    },
    /**
     * Socket Connection
     */

    connectSocket: function () {
        tidioFunc.log('time.0', new Date().getTime() - time_start);
        //
        view.showLoading();

        // Socket connection
        var conn_data = this.connectionData();
        this.socket = io.connect(conn_data.url, this.connection_preferences);

        // Using cache state (to make improvment cache loading)
        var cache_state = this.connectCacheStateGet();

        if (cache_state && !/Android|webOS|iPhone|iPad|iPod|BlackBerry|IEMobile|Opera Mini/i.test(navigator.userAgent)) {
            tidioChat.connectSocketAfterGetStatus(cache_state);
            this.cache_state_used = true;
        }
        // this function should never run with reconnect: false
        this.socket.on('reconnect_attempt', function (a,b) {

            var reconnect_tries = 40;
            if (!tidioChat.getOpenStatus()) {
                reconnect_tries = 3;
            }
            if (a >= reconnect_tries && !tidioChat.disconnect_mode) {
                tidioChat.disconnect();
            }
        });
        this.socket.on('connected', function () {
            tidioFunc.log('socket connected');
            tidioChat.reconnecting.server_down = false;
            $("#tidio-chat-serveroffline").hide();
            tidioChat.reconnecting.reconnect_data = { allow_all: 0, allow_visitor: 0, no_error: 0 };
            tidioChat.connect_state = 'connected'
            if (tidioChat.disconnect_mode) {
                tidioChat.disconnect_mode = false;
                tidioChat.disconnectModeCheck();
            }
            // Connect with socket
            tidioChat.socketVisitorRegister();
        });

        this.socket.on('disconnect', function () {
          // tidioFunc.log('socket', 'disconnect');
            setTimeout(this.tryReconnect.bind(this), this.reconnecting.disconnectBuffer);

        }.bind(this));

        this.socket.on('connect_error', function () {
            // tidioFunc.log('socket', 'connect_error');
            setTimeout(this.tryReconnect.bind(this), this.reconnecting.disconnectBuffer);
            tidioChat.connectionDataCacheClear();
        }.bind(this));

        // Close connection before start
        window.parent.onbeforeunload = function () {
            try {
                if (tidioChat.socket) {
                    tidioChat.socket.disconnect();
                }
            } catch (e) {
            }
        };
    },
    socketVisitorRegister: function (callback, skip_data_changes) {
        callback = tidioFunc._defaultCallback(callback);
        // Get visitor data
        var visitor_data = tidioVisitor.getData();
        var visitor_data_changes = tidioVisitor.getDataChanges();
        tidioFunc.log('visitor_data_changes', visitor_data_changes);
        // Set state to visitor_start && show loading info
        this.connect_state = 'visitor_start';
        view.showLoading();
        // If there is some changes we have to get visitor id
        if(visitor_data_changes && !skip_data_changes){
            this.socketVisitorIdentify(function(visitor_id){
                // If is new visitor id we will reset data
                if(visitor_id){
                    tidioVisitor.setId(visitor_id);
                } else { // If no visitor found, we will back to origin id or create new one
                    var identify_data = tidioVisitor.getDocumentTidioIdentify();
                    // If data like email or distinct_id was provided we will create new id
                    if(identify_data.email || identify_data.distinct_id){
                        tidioVisitor.newId();
                    } else { // If not we have to back to origin id
                        tidioVisitor.resetId();
                    }
                }
                // And call registerAfter again and skip data changes
                this.socketVisitorRegister(callback, true);
            }.bind(this));
        } else {
            this.socketVisitorRegisterAfter(visitor_data, callback);
        }
    },
    socketVisitorIdentify: function(callback) {
        var identify_data = tidioVisitor.getDocumentTidioIdentify();
        this.socket.emit('visitorIdentify', {
            identifyData: identify_data,
            projectPublicKey: this.project_public_key
        }, function (visitor_id) {
            tidioFunc.log('socketVisitorIdentify', visitor_id);
            callback(visitor_id);
        });
    },
    socketVisitorRegisterAfter: function (visitor_data, callback) {
        this.socket.emit('visitorRegister', visitor_data, function (d, err_state) {
            if (err_state) {
                if (err_state == 'INVALID_PROJECT_PUBLIC_KEY') {
                    // Invalid project public key, so we have to delete everydata and reload iframe
                    tidioVisitor.resetId();
                    tidioChat.reloadWidget();
                }
                if (err_state == 'PROJECT_NOT_FOUND') {
                    // PROJECT_NOT_FOUND
                }
                return false;
            }
            if (d.conversationStatus) {
                var storage_status = tidioChat.storage_cookie.get('tidioChatConversationStatus');
                //
                if (!storage_status || d.conversationStatus !== storage_status) { // if conversation status from cookie is different from server we have to update it
                    tidioChat.storage_cookie.set('tidioChatConversationStatus', d.conversationStatus);
                }
            }
            //
            tidioChat.connect_state = 'visitor_finish';
            // Pre-load chat data (1)
            if (!tidioChat.cache_state_used) {
                tidioChat.connectSocketAfterGetStatus(d);
                tidioChat.connectCacheStateSet(d);
            } else if (d.projectStatus != tidioChat.status_true) {

                tidioChat.connectCacheStateSet(d);
                if (d.projectStatus == 'online') {
                    tidioChat.projectStatusGoesOnline();
                }
                if (d.projectStatus == 'offline') {
                    tidioChat.projectStatusGoesOffline();
                }
            }
            // Visitor status (2)
            if (d.visitorStatus && d.visitorStatus == 'banned') {
                tidioChat.applyBan();
                tidioFunc.log('visitor is banned!');
                return false;
            }
            tidioChat.connected = true;
            tidioFunc.log('time.2', new Date().getTime() - time_start);
            if (d.unread_messages) {
                tidioChat.messagesSocketReload();
            }
            callback(true);
            tidioChat.listener('chatIsReadySocket');
            view.hideLoading();
        });
    },
    socketLastSeenUpdate: function (visitor_is_online) {
        var last_seen_data = {
            id: tidioVisitor.getId()
        };
        if (visitor_is_online) {
            last_seen_data.visitorIsOnline = true;
        }
        tidioChat.socket.emit('visitorLastSeenUpdate', last_seen_data, function (data) {
            tidioFunc.log('visitorLastSeenUpdate', data);
            if (data && !data.status && data.value) {
                if (data.value == 'visitorNotRegistered') {
                    tidioChat.visitor_register_limit++;
                    if (tidioChat.visitor_register_limit > 5) {
                        tidioChat.disconnect();
                        return false;
                    }
                    tidioChat.socketVisitorRegister(function (status) {
                        tidioChat.visitor_register_limit = 0;
                    });
                }
            }
        });
    },
    connectSocketAfterGetStatus: function (d) {

        tidioChat.status_true = d.projectStatus;

        if (tidioChat.isOpeningHours()) {
            tidioFunc.log('tidio - opening hours');
            d.projectStatus = 'offline';
            tidioChat.autoRefreshAfterOpeningHours(function () {
                if (tidioChat.status_true && tidioChat.status_true == 'online') {
                    tidioChat.projectStatusGoesOnline();
                }
            });
        }
        tidioChat.isReady(d);
        // (necessary) Run listeners just after init
        tidioChat.connectSocketListeners();
        tidioChat.visitorActivityListener();

        tidioFunc.log('time.1', new Date().getTime() - time_start);
    },

